/**index.wxss**/
.banner image{
  width:100%;
  height:181pt;
}
.title{
  width:100%;
  text-align: center;
  font-size:18px;
  padding:40rpx  0;
}
.service{
  display: flex;
  justify-content:space-between;
  margin-bottom:20rpx;
  position:relative;
}
.service view{
  width:49%;
  height:100%;
}
.service view image{
  width:100%;
}
.service .right{
  
  height:100%;
}
.service .right .bottom{
  width:49%;
  position:absolute;
  right:0;
  bottom:0;
}
.our{
  display: flex;
  justify-content:space-around;
  padding-bottom:20rpx;
}
.our view{
  width:25%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.our view navigator{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.our view image{
  width:50%;

  margin-bottom:20rpx;
}